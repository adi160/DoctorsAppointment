<table id="mytable" class="table table-striped">
  <thead>
    <th>Name</th>
    <th>Age</th>
    <th>sex</th>
    <th>Address</th>
    <th>Time</th>
    <th>Delete</th>
  </thead>
  <tbody>
    <% patient = Patient.find(schedule.patient_id) %>
    <tr>
    <td><%= patient.first_name + " " + patient.last_name %></td>
    <td><%= patient.age %></td>
    <td><%= patient.sex %></td>
    <td><%= patient.address %></td>
    <td><%= schedule.start.strftime("%H:%M:%S %a") %>
    <td><p data-placement="top" data-toggle="tooltip" title="Delete"><button class="btn btn-xs" data-title="Delete" data-toggle="modal" data-target="#delete" ><i class="fa fa-trash"></i></button></p></td>
    </tr>
  </tbody>
</table>